    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "axios": "^1.6.7",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.12.1",
    "lucide-react": "^0.509.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.2",
    "recharts": "^2.15.3",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
