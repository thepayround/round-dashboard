{
  "name": "round-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "node node_modules/prettier/index.cjs --write .",
    "type-check": "tsc --noEmit",
    "validate:components": "node scripts/validate-components.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "audit:security": "npm audit --audit-level=moderate && node scripts/security-scan.js",
    "audit:accessibility": "node scripts/accessibility-check.js",
    "audit:full": "npm run audit:security && npm run audit:accessibility && npm run type-check && npm run lint && npm run test",
    "precommit": "npm run type-check && npm run lint && npm run validate:components"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "flag-icons": "^7.5.0",
    "framer-motion": "^12.12.1",
    "lucide-react": "^0.509.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.5.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-storybook": "^0.11.6",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "vitest": "^1.2.2"
  }
}
